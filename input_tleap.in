source leaprc.gaff2
source leaprc.water.opc
CAGE = loadmol2 CAGE.mol2
PZQ = loadmol2 PZQ.mol2
BFV = loadmol2 BFV.mol2
BFW = loadmol2 BFW.mol2
BFX = loadmol2 BFX.mol2
BFY = loadmol2 BFY.mol2
loadamberparams PZQ.frcmod
loadamberparams BFV.frcmod
loadamberparams BFW.frcmod
loadamberparams BFX.frcmod
loadamberparams BFY.frcmod
loadamberparams CAGE.frcmod

mol = combine { PD1 PD2 LA1 LB1 LC1 LD1 PZQ BFV BFW BFX BFY }
check mol

savepdb mol ALL_dry.pdb
saveamberparm mol ALL_dry.prmtop ALL_dry.inpcrd
loadoff DCMequilbox.lib
solvateBox mol DCMequilbox 12.0
addions mol Na+ 0
addions mol Cl- 0
savepdb mol CAGE_solv.pdb
saveamberparm mol CAGE_solv.prmtop CAGE_solv.inpcrd
quit
 
